    "axes[1, 1].set_ylabel('Accuracy')\n",
    "axes[1, 1].grid(True, alpha=0.3)\n",
    "\n",
    "# 6. Class-wise Confidence\n",
    "class_0_conf = confidences[all_targets == 0]\n",
