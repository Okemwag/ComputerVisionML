    "# Extract features from DeepLabV3+ model\n",
    "deeplabv3_model = models.get('DeepLabV3Plus')\n",
    "if deeplabv3_model is not None:\n",
    "    deeplabv3_model = deeplabv3_model.to(device)\n",
